            android:name="com.example.schoolassistant.IPMTp1"
            android:exported="false" />
        <activity
            android:name="com.example.schoolassistant.INTTp3"
            android:exported="false" />
        <activity
            android:name="com.example.schoolassistant.INTTp2"
            android:exported="false" />
        <activity
            android:name="com.example.schoolassistant.INTTp1"
            android:exported="false" />
        <activity
            android:name="com.example.schoolassistant.CSTp3"
            android:exported="false" />
        <activity
            android:name="com.example.schoolassistant.CSTp2"
            android:exported="false" />
        <activity
            android:name="com.example.schoolassistant.CSTp1"
            android:exported="false" />
        <activity
            android:name="com.example.schoolassistant.ArtITp3"
            android:exported="false" />
        <activity
            android:name="com.example.schoolassistant.ArtITp2"
            android:exported="false" />
        <activity
            android:name="com.example.schoolassistant.ArtITp1"
            android:exported="false" />
        <activity
            android:name="com.example.schoolassistant.MobileAppTp3"
            android:exported="false" />
        <activity
            android:name="com.example.schoolassistant.MobileAppTp2"
            android:exported="false" />
        <activity
            android:name="com.example.schoolassistant.MobileAppTp1"
            android:exported="false" />
        <activity
            android:name="com.example.schoolassistant.PMActivity"
            android:exported="false" />
